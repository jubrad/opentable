<!DOCTYPE html>
<html>
<head>
  <title>Opentable</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% if current_owner %>
    <%= link_to "logout #{current_owner.username}", destroy_owner_session_path, method: :delete %>
 <% else %>
   <%= link_to "login", new_owner_session_path%>
  <% end  %>



<%= yield %>

</body>
</html>
